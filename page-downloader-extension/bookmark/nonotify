javascript:(function(){var d=document,t=new Date().toISOString().replace(/[:.]/g,'-').split('.')[0];function g(r,l){if(l>10)return[];var c=[],w=d.createTreeWalker(r,NodeFilter.SHOW_ELEMENT,null,false),n;while(n=w.nextNode()){if(n.shadowRoot){c.push('\n<!-- SHADOW: '+n.tagName+' -->');c.push(n.shadowRoot.innerHTML);c=c.concat(g(n.shadowRoot,l+1));}}return c;}setTimeout(function(){var h=d.documentElement.outerHTML,s=g(d.documentElement,0);if(s.length>0)h+='\n\n<!-- SHADOW CONTENT -->\n'+s.join('\n');var f=d.querySelectorAll('iframe');for(var i=0;i<f.length;i++){try{var fd=f[i].contentDocument||f[i].contentWindow.document;if(fd)h+='\n\n<!-- IFRAME '+i+' -->\n'+fd.documentElement.outerHTML;}catch(e){h+='\n<!-- IFRAME '+i+': blocked -->\n';}}var st={};try{if(window.__INITIAL_STATE__)st.initialState=window.__INITIAL_STATE__;if(window.__PRELOADED_STATE__)st.preloadedState=window.__PRELOADED_STATE__;if(window.__NEXT_DATA__)st.nextData=window.__NEXT_DATA__;if(Object.keys(st).length>0)h+='\n\n<!-- STATE -->\n<script type="application/json">'+JSON.stringify(st)+'<\/script>';}catch(e){}var b=new Blob([h],{type:'text/html;charset=utf-8'}),u=URL.createObjectURL(b),a=d.createElement('a');a.href=u;a.download=t+'.html';d.body.appendChild(a);a.click();d.body.removeChild(a);URL.revokeObjectURL(u);var sz=(b.size/1024).toFixed(2);console.log('âœ“ Downloaded:',t+'.html','('+sz+' KB)');},1000);})();
